TRUNCATE TABLE Staging_BrandLocation;




Declare @DateFrom as varchar(11), @DateTo  as varchar(11);
SELECT @DateFrom = DateFrom, @DateTo = DateTo FROM DBO.getSyncDateDuration(0);

 

 
 SELECT A.* FROM
(
	SELECT InvoiceDate,  BrandCode, TerritoryCode,  SUM(SoldValue) AS SoldValue 
	FROM  BrandLocation_TerritoryMonthly
	WHERE InvoiceDate BETWEEN   @DateFrom AND @DateTo
	GROUP BY InvoiceDate, BrandCode, TerritoryCode
	
) AS A
LEFT JOIN
(
	

	SELECT  DATEFROMPARTS(YEAR(InvoiceDate), MONTH(InvoiceDate),1) AS InvoiceDate, BrandCode, L.TerritoryCode,  SUM(SoldValue) AS SoldValue 
	FROM  ProductLocation_TerritoryDaily S
		INNER JOIN 
		(
			SELECT * FROM v_SalesLocationAndEmployee 
		) AS L ON S.TerritoryCode = L.TerritoryCode
		INNER JOIN Product P ON S.ProductCode = P.ProductID

	WHERE InvoiceDate BETWEEN @DateFrom AND @DateTo
	GROUP BY  DATEFROMPARTS(YEAR(InvoiceDate), MONTH(InvoiceDate),1), BrandCode, L.TerritoryCode
) AS B ON A.BrandCode =  B.BrandCode AND A.TerritoryCode = B.TerritoryCode AND A.InvoiceDate = B.InvoiceDate
WHERE (A.SoldValue - B.SoldValue) != 0 OR B.BrandCode IS NULL;</property>
