	SELECT  InvoiceDate, TerritoryCode, BrandCode,  SUM(SoldValue) AS SoldValue 
	FROM  ProductLocation_TerritoryDaily S
		INNER JOIN Product P ON S.ProductCode = P.ProductID
	WHERE InvoiceDate BETWEEN @DateFrom AND  @DateTo
	GROUP BY InvoiceDate, TerritoryCode, BrandCode
) AS B ON A.InvoiceDate = B.InvoiceDate AND A.BrandCode =  B.BrandCode AND A.TerritoryCode = B.TerritoryCode
WHERE (A.SoldValue - B.SoldValue) != 0 OR B.TerritoryCode IS NULL</property>
