                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- TRUNCATE STAGING TABLE
 TRUNCATE TABLE Staging_ProductLocation;



Declare @DateFrom as varchar(11), @DateTo  as varchar(11);
SELECT @DateFrom = DateFrom, @DateTo = DateTo FROM DBO.getSyncDateDuration(0);



SELECT A.* FROM
(
	SELECT InvoiceDate, ProductCode, SUM(SoldValue) AS SoldValue 
	FROM  ProductSales_Daily 
	WHERE InvoiceDate  BETWEEN @DateFrom AND @DateTo
	GROUP BY InvoiceDate, ProductCode
	
) AS A
LEFT JOIN
(
	SELECT InvoiceDate, ProductCode, SUM(SoldValue) AS SoldValue 
	FROM  ProductLocation_TerritoryDaily  
	WHERE InvoiceDate  BETWEEN @DateFrom AND @DateTo
	GROUP BY InvoiceDate, ProductCode

) AS B ON A.ProductCode = B.ProductCode AND A.InvoiceDate = B.InvoiceDate

WHERE (A.SoldValue - B.SoldValue) !=0</property>
