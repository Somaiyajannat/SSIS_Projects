                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Declare @DateFrom as varchar(11), @DateTo  as varchar(11);
SELECT @DateFrom = DateFrom, @DateTo = DateTo FROM DBO.getSyncDateDuration();




SELECT A.* 
FROM
(
	SELECT TerritoryCode, InvoiceDate, ProductCode, SUM(SoldQuantity) AS SoldQuantity, SUM(SoldValue) AS SoldValue 
	FROM PROCESS_SERVER.SALES.DBO.ProductLocation_TerritoryDaily
	WHERE InvoiceDate BETWEEN @DateFrom AND @DateTo
	GROUP BY TerritoryCode, InvoiceDate, ProductCode
) AS A
LEFT JOIN
(
	SELECT Z.InvoiceDate,TerritoryCode, Z.ProductCode, ISNULL(SUM(Z.SoldQuantity),0) AS SoldQuantity, SUM(SoldValue) AS SoldValue
	FROM (

	--TRANSIT 
		SELECT  CL.TerritoryCode, I.InvoiceDate, ID.ProductCode, SUM(ID.InvoiceQty) AS SoldQuantity, SUM(ID.TotalTP) AS SoldValue
			FROM SD_DMS.DBO.Invoice I  WITH(NOLOCK) 

			LEFT JOIN SD_DMS.DBO.InvoiceDetail ID  WITH(NOLOCK) ON I.InvoiceCode = ID.InvoiceCode
			LEFT JOIN SD_DMS.DBO.SummaryInvoice SI WITH(NOLOCK)  ON I.InvoiceCode = SI.InvoiceCode 
			LEFT JOIN v_ChemistAndLocation CL ON I.ChemistCode = CL.ChemistCode

			WHERE SI.SummaryCode IS NULL 
			AND InvoiceDate BETWEEN @DateFrom AND  @DateTo
			AND I.IsActive = 1 
			AND ID.IsActive = 1
			GROUP BY  I.InvoiceDate, ID.ProductCode, CL.TerritoryCode


			--SOLD
			UNION ALL 

			SELECT  CL.TerritoryCode,I.InvoiceDate, SID.ProductCode, SUM(SID.SoldQty) AS SoldQuantity, SUM(SID.TotalTP) AS SoldValue
			
			FROM  SD_DMS.DBO.Invoice I   WITH(NOLOCK) 
			INNER JOIN SD_DMS.DBO.SummaryInvoice SI  WITH(NOLOCK) ON SI.InvoiceCode = I.InvoiceCode 
			INNER JOIN SD_DMS.DBO.Summary S ON SI.SummaryCode = S.SummaryCode
			INNER JOIN SD_DMS.DBO.SummaryInvoiceDetail SID WITH(NOLOCK)  ON SI.InvoiceCode = SID.InvoiceCode 
			LEFT JOIN v_ChemistAndLocation CL ON I.ChemistCode = CL.ChemistCode

			WHERE  InvoiceDate BETWEEN @DateFrom AND  @DateTo
			AND I.IsActive = 1 
			AND SI.IsActive = 1
			AND S.IsActive = 1
			AND SID.IsActive = 1
			GROUP BY I.InvoiceDate, SID.ProductCode, CL.TerritoryCode

	)  AS Z group by ProductCode, InvoiceDate, TerritoryCode
) AS B ON A.ProductCode = B.ProductCode AND A.TerritoryCode = B.TerritoryCode AND A.InvoiceDate = B.InvoiceDate

WHERE (A.SoldValue  - B.SoldValue) !=0</property>
