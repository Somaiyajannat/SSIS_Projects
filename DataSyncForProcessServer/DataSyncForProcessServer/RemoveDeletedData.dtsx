                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Declare @DateFrom as varchar(11), @DateTo  as varchar(11);
SELECT @DateFrom = DateFrom, @DateTo = DateTo FROM DBO.getSyncDateDuration(0);


SELECT A.* FROM
(
	SELECT  InvoiceDate, ProductCode,  SUM(SoldValue) AS SoldValue 
	FROM  ProductLocation_TerritoryDaily T
		INNER JOIN TerritoryMaster L ON T.TerritoryCode = L.TerritoryCode
	WHERE InvoiceDate BETWEEN @DateFrom AND @DateTo
	GROUP BY InvoiceDate, ProductCode
) AS A
LEFT JOIN
(
	SELECT  InvoiceDate, ProductCode, SUM(SoldValue) AS SoldValue 
	FROM  ProductLocation_AreaDaily 
	WHERE InvoiceDate  BETWEEN @DateFrom AND @DateTo
	GROUP BY  InvoiceDate, ProductCode
) AS B ON A.InvoiceDate = B.InvoiceDate AND A.ProductCode =  B.ProductCode
WHERE (A.SoldValue - B.SoldValue) != 0</property>
