                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- TRUNCATE STAGING TABLE
TRUNCATE TABLE Staging_LocationSales;


Declare @DateFrom as varchar(11), @DateTo  as varchar(11);
SELECT @DateFrom = DateFrom, @DateTo = DateTo FROM DBO.getSyncDateDuration(0);


SELECT A.* FROM
(
	SELECT   InvoiceDate, AreaCode,  SUM(SoldValue) AS SoldValue 
	FROM  LocationSales_AreaDaily 
	WHERE InvoiceDate BETWEEN @DateFrom AND  @DateTo
	GROUP BY InvoiceDate, AreaCode
	
) AS A
LEFT JOIN
(

	SELECT  InvoiceDate, AreaCode,  SUM(SoldValue) AS SoldValue 
	FROM  ProductLocation_AreaDaily S
	WHERE InvoiceDate BETWEEN @DateFrom  AND  @DateTo
	GROUP BY  InvoiceDate, AreaCode
	
) AS B ON A.AreaCode = B.AreaCode AND A.InvoiceDate = B.InvoiceDate
WHERE (A.SoldValue - B.SoldValue) != 0 OR B.AreaCode IS NULL;</property>
