) AS A
LEFT JOIN
(
	

	SELECT  DATEFROMPARTS(YEAR(InvoiceDate), MONTH(InvoiceDate),1) AS InvoiceDate, BrandCode, L.RegionCode,  SUM(SoldValue) AS SoldValue 
	FROM  ProductLocation_TerritoryDaily S
		INNER JOIN 
		(
			SELECT * FROM v_SalesLocationAndEmployee 
		) AS L ON S.TerritoryCode = L.TerritoryCode
		INNER JOIN Product P ON S.ProductCode = P.ProductID

	WHERE InvoiceDate BETWEEN @DateFrom AND @DateTo
	GROUP BY  DATEFROMPARTS(YEAR(InvoiceDate), MONTH(InvoiceDate),1), BrandCode, L.RegionCode
) AS B ON A.BrandCode =  B.BrandCode AND A.RegionCode = B.RegionCode AND A.InvoiceDate = B.InvoiceDate
WHERE (A.SoldValue - B.SoldValue) != 0 OR B.BrandCode IS NULL;</property>
