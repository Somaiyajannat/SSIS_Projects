                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Declare @FromDate as varchar(11) = SD_DMS.DBO.getCurrentMonthFirstDate(GETDATE());
Declare @ToDate as varchar(11) =  DATEADD(DAY,-1, GETDATE());



	SELECT   @FromDate AS FromDate, @ToDate AS ToDate, A.* FROM
	(
		SELECT DepotCode,TerritoryCode,  SUM(SoldValue) AS SoldValueInHO
		FROM (

		--TRANSIT 
			SELECT    DA.DepotCode,CL.TerritoryCode,  SUM(ID.TotalTP) AS SoldValue
				FROM SD_DMS.DBO.Invoice I  WITH(NOLOCK) 

				LEFT JOIN SD_DMS.DBO.InvoiceDetail ID  WITH(NOLOCK) ON I.InvoiceCode = ID.InvoiceCode
				LEFT JOIN SD_DMS.DBO.SummaryInvoice SI WITH(NOLOCK)  ON I.InvoiceCode = SI.InvoiceCode 
				LEFT JOIN SD_DMS.DBO.v_ChemistAndLocation CL ON I.ChemistCode = CL.ChemistCode
				LEFT JOIN DepotAreaMapping DA ON CL.AreaCode = DA.AreaCode

				WHERE SI.SummaryCode IS NULL AND
				InvoiceDate BETWEEN @FromDate AND  @ToDate
				AND I.IsActive = 1 
				AND ID.IsActive = 1

				GROUP BY   DA.DepotCode, CL.TerritoryCode


				--SOLD
				UNION ALL 

				SELECT  DA.DepotCode, CL.TerritoryCode,  SUM(SID.TotalTP) AS SoldValue
			
				FROM  SD_DMS.DBO.Invoice I   WITH(NOLOCK) 
				INNER JOIN SD_DMS.DBO.SummaryInvoice SI  WITH(NOLOCK) ON SI.InvoiceCode = I.InvoiceCode 
				INNER JOIN SD_DMS.DBO.Summary S WITH(NOLOCK) ON SI.SummaryCode = S.SummaryCode
				INNER JOIN SD_DMS.DBO.SummaryInvoiceDetail SID WITH(NOLOCK)  ON SI.InvoiceCode = SID.InvoiceCode 
				INNER JOIN SD_DMS.DBO.v_ChemistAndLocation CL ON I.ChemistCode = CL.ChemistCode
				INNER JOIN DepotAreaMapping DA ON CL.AreaCode = DA.AreaCode

				WHERE  InvoiceDate BETWEEN @FromDate AND  @ToDate
				AND I.IsActive = 1 
				AND SI.IsActive = 1
				AND S.IsActive = 1
				AND SID.IsActive = 1
			
				GROUP BY  DA.DepotCode, CL.TerritoryCode

		)  AS Z 
	
		GROUP BY DepotCode,TerritoryCode
	
	) A

WHERE A.DepotCode = ?</property>
