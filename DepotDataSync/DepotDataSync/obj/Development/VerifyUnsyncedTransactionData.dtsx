                          name="OpenRowsetVariable"></property>
                        <property
                          dataType="System.String"
                          description="The SQL command to be executed."
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT    S.SummaryCode, IsActive, SI.NumberOfInvoice, SID.NumberOfInvoiceDetail,SDP.NumberOfInvoiceProduct,  SC.NumberOfCollection, SCD. NumberOfCollectionDetail
 
FROM      SD_DMS.DBO.Summary S WITH (NOLOCK)

-- SUMMARY INVOICE
LEFT JOIN 
(
	SELECT SummaryCode, COUNT(InvoiceCode) AS NumberOfInvoice FROM SummaryInvoice  WITH (NOLOCK)
	GROUP BY SummaryCode
) AS SI ON S.SummaryCode = SI.SummaryCode 

-- SUMMARY INVOICE DETAIL
LEFT JOIN 
(
	SELECT SummaryCode, COUNT(InvoiceCode) AS NumberOfInvoiceDetail FROM SummaryInvoiceDetail WITH (NOLOCK)
	GROUP BY SummaryCode
) AS SID ON S.SummaryCode = SID.SummaryCode 

-- SUMMARY INVOICE PRODUCT BATCH WISE
LEFT JOIN 
(
	SELECT SummaryCode, COUNT(InvoiceCode) AS NumberOfInvoiceProduct FROM SummaryInvoiceProductBatchWise WITH (NOLOCK)
	GROUP BY SummaryCode
) AS SDP ON S.SummaryCode = SDP.SummaryCode 

-- SUMMARY COLLECTION
LEFT JOIN 
(
	SELECT SummaryCode, COUNT(CollectionCode) AS NumberOfCollection FROM SummaryCollection  WITH (NOLOCK)
	GROUP BY SummaryCode
) AS SC ON S.SummaryCode = SC.SummaryCode 


-- SUMMARY COLLECTION DETAIL
LEFT JOIN 
(
	SELECT SummaryCode, COUNT(SCD.CollectionCode) AS NumberOfCollectionDetail
	FROM SummaryCollectionDetail SCD  WITH (NOLOCK) 
		INNER JOIN SummaryCollection SC WITH (NOLOCK)  ON SCD.CollectionCode = SC.CollectionCode
	GROUP BY SummaryCode
) AS SCD ON S.SummaryCode = SCD.SummaryCode 

WHERE       SummaryDate&gt;=DATEADD(DAY, -31,GETDATE())

ORDER BY CreatedOn DESC</property>
