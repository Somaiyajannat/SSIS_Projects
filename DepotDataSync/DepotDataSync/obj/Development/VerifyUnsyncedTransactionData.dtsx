-- INVOICE DETAIL
LEFT JOIN 
(
	SELECT InvoiceCode, COUNT(ProductCode) AS NumberOfProduct FROM InvoiceDetail  WITH (NOLOCK)
	GROUP BY InvoiceCode
) AS ID ON I.InvoiceCode = ID.InvoiceCode 

-- INVOICE DETAIL PRODUCT
