                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Declare @DateFrom as varchar(11)= SD_DMS.DBO.getLastMonthFirstDate(GETDATE()) , @DateTo  as varchar(11)=  GETDATE();


SELECT A.*, I.IsActive FROM
(
	SELECT  I.InvoiceCode,  SUM(ID.Amount) AS InvoiceValue
			
	FROM  SD_DMS.DBO.Invoice I   WITH(NOLOCK) 
	INNER JOIN SD_DMS.DBO.InvoiceDetail ID WITH(NOLOCK)  ON I.InvoiceCode = ID.InvoiceCode 
	INNER JOIN v_ChemistAndLocation CL ON I.ChemistCode = CL.ChemistCode
	INNER JOIN DepotAreaMapping DA ON CL.AreaCode = DA.AreaCode
	LEFT JOIN SD_DMS.DBO.SummaryInvoice SI  WITH(NOLOCK) ON SI.InvoiceCode = I.InvoiceCode 

	WHERE  InvoiceDate BETWEEN @DateFrom AND @DateTo
	AND I.IsActive = 1 
	AND SI.SummaryCode IS NULL
	AND ID.IsActive = 1
	AND DA.DepotCode = ?
	GROUP BY I.InvoiceCode

	

) AS A 
INNER JOIN SD_DMS.DBO.Invoice I ON A.InvoiceCode = I.InvoiceCode</property>
