                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT ID.* FROM Invoice I
	INNER JOIN InvoiceDetail ID ON I.InvoiceCode = ID.InvoiceCode
	INNER JOIN 
	(
		SELECT MC.ChemistCode, DA.DepotCode FROM MarketChemists MC
		INNER JOIN TerritoryMarket TM ON MC.MarketCode = TM.MarketCode
		INNER JOIN TerritoryMaster T ON TM.TerritoryCode = T.TerritoryCode
		INNER JOIN DepotAreaMapping DA ON T.AreaCode =DA.AreaCode
	) AS CD ON I.ChemistCode = CD.ChemistCode

	WHERE I.InvoiceDate BETWEEN '2022-12-01' AND '2022-12-09'
